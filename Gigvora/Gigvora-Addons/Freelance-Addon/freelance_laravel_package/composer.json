{
    "name": "taskup/freelance-laravel-package",
    "description": "Freelance gig, project, dispute, and escrow module extracted from Taskup as a reusable Laravel package.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "laravel/framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Taskup\\Freelance\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Taskup\\Freelance\\FreelanceServiceProvider"
            ]
        }
    }
}
