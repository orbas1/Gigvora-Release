{
  "name": "advertisement/package",
  "description": "Modular Laravel advertisement package with campaign management, placements, targeting, forecasting, and affiliate support derived from Sngine.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Advertisement Package Maintainers",
      "email": "dev@example.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/database": "^10.0|^11.0",
    "illuminate/auth": "^10.0|^11.0",
    "illuminate/routing": "^10.0|^11.0",
    "illuminate/validation": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Advertisement\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Advertisement\\Providers\\AdvertisementServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
