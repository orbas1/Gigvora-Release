{
    "name": "gigvora/talent-ai",
    "description": "Talent AI addon for headhunters, launchpad, volunteering, and AI workspace",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/auth": "^9.0|^10.0|^11.0",
        "illuminate/routing": "^9.0|^10.0|^11.0",
        "illuminate/events": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Gigvora\\TalentAi\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gigvora\\TalentAi\\Providers\\TalentAiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
