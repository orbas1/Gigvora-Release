{
    "name": "gigvora/ai-headhunter-launchpad-addon",
    "description": "Talent AI addon for headhunters, launchpad, volunteering, and AI workspace",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/auth": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0",
        "illuminate/events": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Gigvora\\TalentAi\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gigvora\\TalentAi\\Providers\\TalentAiServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
